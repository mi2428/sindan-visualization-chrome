{
  "name": "SINDAN Visualization",
  "version": "1.0",
  "manifest_version": 3,
  "description": "For ShowNet 2022",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": [
      "content.js"
    ]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "/icons/sindan.png"
  },
  "action": {}
}
